<?xml version="1.0" encoding="utf-8"?>
<odoo>

<template id="s_title" inherit_id="website.s_title">
    <!-- Section -->
    <xpath expr="//section" position="attributes">
        <attribute name="class" add="pt160 pb160 o_cc o_cc2" remove="pt40 pb40" separator=" "/>
        <attribute name="data-oe-shape-data">{'shape':'web_editor/Floats/04','colors':{'c1':'#F9BE24','c2':'#42D29B','c4':'#41BFF7','c5':'#F84F51'},'flip':[],'showOnMobile':false,'shapeAnimationSpeed':'0','animated':'true'}</attribute>
    </xpath>
    <!-- Shape -->
    <xpath expr="//div[hasclass('container')]" position="before">
        <div class="o_we_shape o_web_editor_Floats_04 o_we_animated" style="background-image: url('/web_editor/shape/web_editor%2FFloats%2F04.svg?c1=%23F9BE24&amp;c2=%2342D29B&amp;c4=%2341BFF7&amp;c5=%23F84F51');"/>
    </xpath>
    <!-- Title -->
    <xpath expr="//h2" position="replace" mode="inner">
        Reading means borrowing
    </xpath>
</template>

</odoo>
